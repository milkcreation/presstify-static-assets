{
  "name": "presstify-plugins/static-assets",
  "description": "Gestion de ressource medias client/server.",
  "type": "presstify-plugin",
  "keywords": [
    "presstify",
    "plugins",
    "assets",
    "glide",
    "flysystem"
  ],
  "homepage": "https://github.com/milkcreation/presstify-static-assets.git",
  "time": "2021-09-14",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy.manner@milkcreation.fr",
      "role": "Developper"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "presstify/framework": ">=1.4",
    "league/flysystem-memory": "^1.0",
    "league/glide": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "tiFy\\Plugins\\StaticAssets\\": ""
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config" : {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "secure-http": false,
    "sort-packages": true
  }
